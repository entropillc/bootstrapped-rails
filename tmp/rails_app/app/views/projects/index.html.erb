<% title "Projects" %>

<div class="well">
	<table class="zebra-striped">
	  <tr>

	    <th>Name</th>
	  </tr>
	  <% @projects.each do |project| %>
	    <tr>

	      <td><%= project.name %></td>
	    </tr>
	  <% end %>
	</table>
</div>
<p><%= link_to "New Project", new_project_path, :class => 'btn primary float-right' %></p>
